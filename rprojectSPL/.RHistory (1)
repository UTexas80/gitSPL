install.packages("tidyquant") # Loads tidyquant, tidyverse, lubridate, xts, quantmod, TTR  library(ggplot2) library(tidyquant) # Use FANG data set data("FANG") # Get SPL Stock Prices SPL <- tq_get("SPL.AX", get = "stock.prices", from = "2015-09-01", to = "2017-10-30")
# Loads tidyquant, tidyverse, lubridate, xts, quantmod, TTR  library(ggplot2) library(tidyquant) # Get SPL Stock Prices SPL <- tq_get("SPL.AX", get = "stock.prices", from = "2010-05-19", to = "2017-10-30") # Setup dates for zoom window end <- ymd("2017-10-30") start <- end - weeks(20) # Create chart SPL %>%   ggplot(aes(x = date, y = close)) +   geom_line() +   labs(title = "SPL: Line Chart the Previous Way",        subtitle = "This is good, but doesn't give us open, high, low, or direction information",        x = "", y = "Closing Price") +   coord_x_date(xlim = c(start, end),                ylim = c(700, 870))
